{% form_theme form with [
'ZikulaFormExtensionBundle:Form:bootstrap_3_zikula_admin_layout.html.twig',
'ZikulaFormExtensionBundle:Form:form_div_layout.html.twig'
] %}
{{ polyfill() }}
{{ adminHeader() }}

<h2><i class="fa fa-wrench"></i> {{ method.DisplayName }} {{ __('Settings') }}</h2>
<div class="well">
    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_end(form) }}
</div>

{{ adminFooter() }}
